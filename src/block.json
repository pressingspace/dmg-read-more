{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dmg/read-more",
	"title": "Read More [dmg::media]",
	"category": "widgets",
	"description": "A “Read More” link to a post.",
	"icon": "admin-links",
	"keywords": [ "link" ],
	"version": "0.1.0",
	"textdomain": "dmg-read-more",
	"attributes": {
		"linkedPostId": {
			"type": "number",
			"default": 0
		},
		"linkedPostType": {
			"type": "string",
			"default": "post"
		}
	},
	"selectors": {
		"root": ".wp-block-dmg-read-more"
	},
	"example": {},
	"supports": {
		"html": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
